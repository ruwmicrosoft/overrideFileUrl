						"commands": [
							{
								"description": "Search Jira for tickets, issues, epics, stories, tasks, sub tasks, bugs",
								"id": "jiraSearch",
								"initialRun": true,
								"parameters": [
									{
										"description": "Issue key or Keyword",
										"name": "searchKeyword",
										"title": "Keywords"
									},
									{
										"description": "A precise JQL query generated from the utterance. This is a mandatory parameter. Use JQL instructions for generation",
										"name": "jql",
										"title": "JQL"
									},
									{
										"description": "Type can be one these: 'Bug','Task','Epic', 'Story'. StatusCategory must be one of these: 'To Do','In Progress','Done'",
										"name": "jqlIns1",
										"title": "JQL Instructions Part 1",
										"value": "Allowed priority values are 'Lowest','Low','Medium','High','Highest'. Multiple Priority values can be selected for a given query. Ex. Priority in ('High','Highest')"
									},
									{
										"description": "Assignee value must be in single quotes. Eg.Assignee='john@xyz.com'. If self assigned, always use Assignee=currentUser()",
										"name": "jqlIns2",
										"title": "JQL Instructions Part 2"
									},
									{
										"description": "Project,Summary,Text must be in quotes. Eg. Text~'abc' AND Project='xyz'. For linked items use:issue in linkedIssues()",
										"name": "jqlIns3",
										"title": "JQL Instructions Part 3"
									}
								],
